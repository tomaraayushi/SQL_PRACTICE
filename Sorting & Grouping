--Q1
select
    teacher_id,
    count(distinct subject_id) as cnt
from Teacher
group by teacher_id;

--Q2
select
    activity_date as day,
    count(distinct user_id)as active_users
from Activity
where activity_date between ('2019-07-27'::date - interval '29' day) and '2019-07-27'
group by activity_date;
